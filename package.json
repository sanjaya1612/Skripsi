{
  "name": "travel.id",
  "version": "1.0.0",
  "description": "MERN Travel.id app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "nodemon --exec babel-node src/backend/server.js",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanjaya1612/Skripsi.git"
  },
  "author": "Sanjaya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sanjaya1612/Skripsi/issues"
  },
  "homepage": "https://github.com/sanjaya1612/Skripsi#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.14",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "react-number-format": "^4.9.1"
  },
  "devDependencies": {
    "concurrently": "^6.5.1",
    "nodemon": "^2.0.15"
  }
}
